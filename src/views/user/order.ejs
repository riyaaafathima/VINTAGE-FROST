

<style>
    .order-card {
        border: 1px solid #dee2e6;
        border-radius: 8px;
        margin-bottom: 1rem;
        padding: 1rem;
        background-color: white;
    }
    
    .product-img {
        width: 100px;
        height: 100px;
        object-fit: cover;
    }
    
    .status-pending {
        color: #ffc107;
    }
    
    .status-returned {
        color: #dc3545;
    }
    
    .main-container {
        max-width: 800px;
        margin: 2rem auto;
        padding: 0 1rem;
    }
    
    .custom-color {
        color: #AD9170;
    }
    
    .price {
        color: #AD9170;
        font-weight: bold;
    }
    
    .view-btn {
        background-color: #AD9170;
        border: none;
        padding: 0.25rem 1rem;
        font-size: 0.875rem;
    }
    
    .view-btn:hover {
        background-color: #8a745a;
    }




    /* /////// review modal */
    :root {
      --background-color: #F1EFE7;
      --button-color: #AD9170;
    }
    
    body {
      background-color: #f8f9fa;
    }
    
    .modal-content {
      background-color: var(--background-color);
      border-radius: 15px;
    }
    
    .modal-header {
      border-bottom: none;
      padding-bottom: 0;
    }
    
    .modal-footer {
      border-top: none;
      padding-top: 0;
    }
    
    .btn-custom {
      background-color: var(--button-color);
      color: white;
      border: none;
      padding: 10px 25px;
      border-radius: 8px;
      font-weight: 500;
      transition: all 0.3s ease;
    }
    
    .btn-custom:hover {
      background-color: #97806A;
      color: white;
    }
    
    .star-rating {
      direction: rtl;
      display: inline-block;
      padding: 20px 0;
    }
    
    .star-rating input {
      display: none;
    }
    
    .star-rating label {
      color: #ddd;
      font-size: 30px;
      padding: 0 5px;
      cursor: pointer;
      transition: all 0.3s ease;
    }
    
    .star-rating label:hover,
    .star-rating label:hover ~ label,
    .star-rating input:checked ~ label {
      color: #FFD700;
    }
    
    textarea.form-control {
      background-color: rgba(255, 255, 255, 0.7);
      border: 1px solid #ddd;
      border-radius: 8px;
      resize: none;
    }
    
    .rating-value {
      font-size: 18px;
      font-weight: 500;
      color: #666;
      margin-top: 10px;
    }
</style>

